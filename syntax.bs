#include libs/stdlib.bs
#include libs/posix.bs

block recursive int -> void
    cur
    stdout_i cur
    cur + 1
    if cur == 10
    | return
    recursive cur
end

block main void -> int
    i = 0
    label hi
    i + 1
    stdout_i i
    if i != 10
    | goto hi

    return 0
end